{
    "opts": {
        "encoding": "utf8",
        "destination": "docs",
        "packageX": "package.json",
        "readme": "README.md",
        "recurse": true
    },
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": [ "resources" ],
        "includePattern": ".+\\.js$"
    },
    "tags": {},
